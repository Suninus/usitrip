/* 收藏成功的提示 */
.addSuccess .btn{ margin-right:20px;}
.addSuccess div#Favorites_Content{color:#F7860F;}

/* 搜索结果为空的情况 */
.notFound{ border-top:0px; overflow:hidden;border-bottom: 1px solid #C5E6F9;}
.notFound .topTip{ height:80px; padding-left:150px; background:#FFFCF5; background:url(/image/no_found.jpg) no-repeat 80px 15px #FFFCF5; line-height:80px; font-size:14px; border-bottom: 1px solid #F7E5B3;border-top: 1px solid #F7E5B3;}
.notFound .suggest{ margin:10px 25px; line-height:24px;}
.notFound .suggest h3{ font-size:14px;}
.notFound .phone{ margin-bottom:15px; line-height:20px;}
.notFound .phone li{ display:inline-block; *display:inline; *zoom:1; width:100%; margin-left:10px; padding-left:22px; background:url(/image/icons/phone_blue.gif) left center no-repeat;}
.notFound .phone li.first{ width:200px;}
.notFound .phone li.service{ width:auto; background:url(/image/icons/icon_flag_4.gif) left center no-repeat;}
.notFound .message{ margin:5px 10px 15px 12px;}
.notFound .message li{ margin:4px 0;}
.notFound .message label{ display:inline-block; *display:inline; *zoom:1;  width:65px; color:#777; vertical-align:top; }
.notFound .message .text{ width:180px;}
.notFound .message .textarea{ width:385px; height:65px;}
.notFound .message span{ display:inline-block; *display:inline; *zoom:1; margin-left:10px; vertical-align:top; color:#777;  }


/* 您可能喜欢 */
