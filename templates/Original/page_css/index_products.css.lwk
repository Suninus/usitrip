.suggestRoute ul{ border:1px solid #C5E6F9; border-top:0px; overflow:hidden;}
.suggestRoute li{ width:100%; padding:12px 20px 10px; border-bottom:1px solid #efefef; overflow:hidden; line-height:18px;}
.suggestRoute li.bottom{ border:0;}
