function getHistory(){
	<?php echo $js_days;?>
	var sh = lwkai.getCookie('search_history','<?php echo CHARSET?>');
	if (!sh) return;
	jQuery('.desthelp').css('visibility','visible');
	var arrs = sh.split('|');
	var search_history = new Array();
	
	for(var $i = 0, len = arrs.length; $i < len; $i++){
		var _index = search_history.length;
		search_history[_index] = new Array();
		var temp = arrs[$i].split(',');
		for (var j = 0, len_j = temp.length; j < len_j; j++) {
			var keyval = temp[j].split(':');
			search_history[_index][keyval[0]] = keyval[1];
		}
	}
	var html = '';
	search_history.reverse();
	for (var $i = 0, len = search_history.length; $i < len; $i++) {
		if ($i > 9) break;
		html += '<p><a href=\"<?php echo tep_href_link('advanced_search_result.php','')?>' + 
			'?fcw=' + encodeURIComponent(search_history[$i]['fcw']) + 
			'&tcw=' + encodeURIComponent(search_history[$i]['tcw']) + 
			'&d=' + encodeURIComponent(search_history[$i]['d']) + 
			'&w=' + encodeURIComponent(search_history[$i]['w']) + 
			'&u=true\"><nobr>';
		html += (!!search_history[$i]['fcw'] ? '<?php echo db_to_html('出发地:')?>' + search_history[$i]['fcw'] : ''); 
		html += (!!search_history[$i]['tcw'] ? ' <?php echo db_to_html(' 目的地:')?>' + search_history[$i]['tcw'] : ''); 
		html += (!!search_history[$i]['d'] ? ' <?php echo db_to_html(' 天数:')?>' + js_day[search_history[$i]['d']] : ''); 
		html += (!!search_history[$i]['w'] ? ' <?php echo db_to_html(' 关键字:')?>' + search_history[$i]['w'] : '');
		html += '</nobr></a></p>';
		
	}
	jQuery('#html').html(html);
	var width = 0;
	var obj = jQuery('#html > a');
}


jQuery(document).ready(function(){
	getHistory();/* 调用搜索记录 */
	jQuery('.desthelp').hover(function(){
		jQuery(this).children().eq(1).fadeIn(100);
	},
