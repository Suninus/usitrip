	jQuery("#fcw").click(function(){
		jQuery(this).removeClass('inputcolordefault');
		if (jQuery(this).val() == jQuery(this).attr('deftxt')) jQuery(this).val('');

		if (jQuery('#tcw').val().replace(jQuery('#tcw').attr('deftxt'),'').length == 0) {
			jQuery('#fcw_panel .pop_cityBegin').css('display','block'); 
			jQuery('#fcw_panel .pop_cityEnd').css('display','none');
		} else {
			jQuery('#fcw_panel .pop_cityBegin').css('display','none'); 
			jQuery('#fcw_panel .pop_cityEnd').css('display','block');
		}
		lwkai.maskSelect('fcw_panel','tcw_panel');
	});
