	jQuery("#divPlaceStartCloseBtn_1,#divPlaceStartCloseBtn_2").click(function(){
		lwkai.clearMaskSelect('fcw_panel');
		lwkai.inputDefTxt('fcw','inputcolordefault');
	});
	/* document click clear float div */
	jQuery("html").bind("click", function(){ 
		lwkai.clearMaskSelect('fcw_panel');
		lwkai.inputDefTxt('fcw','inputcolordefault');
	});
	/*  点击其中的某个A元素 */
	jQuery("#fcw_panel dl dd a").click(function(){
		var val = jQuery(this).text();
		var oldval = jQuery('#fcw').val();
		lwkai.clearMaskSelect('fcw_panel');
		val = val ? val.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		oldval = oldval ?  oldval.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		if (val != oldval) {
			jQuery("#fcw").val(val);
			jQuery('#fcw').change();
		}
	});
	/* 出发地输入框change事件 */
	jQuery('#fcw').change(function(){
		var url = jQuery(this).attr('dataurl');
		_sendAjax(url);
	});
	/* 目的地输入框change事件 */
	jQuery('#tcw').change(function(){
		var url = jQuery(this).attr('dataurl');
		_sendAjax(url);
	});
	
	/* AJAX提交搜索框内用户改变的值 返回对应显示的值 */
	function _sendAjax(url){
        var fcw = jQuery('#fcw').val();
		var tcw = jQuery('#tcw').val();
		var _charset = '<?php echo strtolower(CHARSET);?>';
		if (_charset == 'gb2312') {
			fcw = simplized(fcw);
			tcw = simplized(tcw);
		} else {
			fcw = traditionalized(fcw);
			tcw = traditionalized(tcw);
		}
		fcw = fcw ? fcw.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		var fcwDeftxt = jQuery('#fcw').attr('deftxt');
		fcwDeftxt = fcwDeftxt != undefined ? fcwDeftxt.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		
		fcw = fcw == fcwDeftxt ? '' : fcw;
		
		tcw = tcw ? tcw.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		var tcwDeftxt = jQuery('#tcw').attr('deftxt');
		tcwDeftxt = tcwDeftxt != undefined ? tcwDeftxt.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		
		tcw = tcw == tcwDeftxt ? '' : tcw;
		
		var d = jQuery('#d2').val();
		var action = url.split('?');
		action = action[1].split('=');
		action = action[1];
		if (url == undefined  || url.replace(/^\s+/,'').replace(/\s+$/,'') == '') {
			alert('Error!\nNo definition dataurl attributes!');
			return; 	
		}
		
		if ((fcw != '' && fcw != fcwDeftxt && action =='get_end_city') || (tcw !='' && tcw != tcwDeftxt && action =='get_start_city') || (d != '' && action == 'get_day_type')) {
			jQuery.post(url,{'fcw' : fcw, 'tcw' : tcw, 'd' : d},function(r){
			<?php
			$_days = array();
			
			foreach($_SEARCH_DATA['Days'] as $key => $val){
				$_days[$key] = array('id'=>$val['id'], 'name'=>iconv(CHARSET, 'utf-8',$val['name']));
			}
			?>
			var day = <?php echo json_encode($_days)?>;
			function _find(id,arr){
				for(tt in arr) {
					if (id == arr[tt].id) {
						return arr[tt];
					}
				}
				return false;
			}
/*			startCity  出发地
			endCity 目的地
			dayType 天数
			 如果出发地返回了数据 则需要更新页面数据
*/
			if (r['startCity'].length > 0){
				jQuery('#fcw_panel .pop_cityEnd dl dd').html();
				var temp = '';
				for(var i =0, len = r['startCity'].length; i < len; i++) {
					temp += '<a href="javascript:void(0)">' + r['startCity'][i] + '</a>';
				}
				jQuery('#fcw_panel .pop_cityEnd dl dd').html(temp);
				/* 补上所有点击事件 */
				jQuery("#fcw_panel .pop_cityEnd dl dd a").bind('click',function(){
					var val = jQuery(this).text();
					var oldval = jQuery('#fcw').val();
					lwkai.clearMaskSelect('fcw_panel');
					val = val ? val.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
					oldval = oldval ?  oldval.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
					if (val != oldval) {
						jQuery("#fcw").val(val);
						jQuery('#fcw').change();
					}
				});
	
			}
			
			/*如果目的地返回了数据  则更新页面数据*/
			if (r['endCity'].length > 0) {
				jQuery('#tcw_panel .pop_cityEnd dl dd').html();
				var temp = '';
				for(var i =0, len = r['endCity'].length; i < len; i++) {
					temp += '<a href="javascript:void(0)">' + r['endCity'][i] + '</a>';
				}
				jQuery('#tcw_panel .pop_cityEnd dl dd').html(temp);
				/*  补上所有点击事件 
				  点击其中的某个A元素*/ 
				jQuery("#tcw_panel dl dd a").bind('click',function(){
					var val = jQuery(this).text();
					var oldval = jQuery('#tcw').val();
					lwkai.clearMaskSelect('tcw_panel');
					val = val ? val.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
					oldval = oldval ?  oldval.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
					if (val != oldval) {
						jQuery("#tcw").val(val);
						jQuery('#tcw').change();
					}
				});
			}
			
			if (r['dayType'].length > 0) {
				var oldval = jQuery('#d2').val();
				jQuery('#d2').empty();
				var temp = '<option value="' + day['0']['id'] + '">' + day['0']['name'] + '</option>';
				var id,text,tmp_obj;
				for (var i = 0,len = r['dayType'].length; i < len; i++) {
					tmp_obj = _find(r['dayType'][i],day);
					id = tmp_obj.id;
					text = tmp_obj.name;
					temp +=  '<option value="' + id + '">' + text + '</option>';
				}
				jQuery('#d2').html(temp).addClass('inputcolordefault');
				jQuery('#d2').val(oldval);
				jQuery('#d2').val() != '' ? jQuery('#d2').removeClass('inputcolordefault') : '';
			}
		
		},'json');
		}
    };
	
	
	/* 目的地输入框 */
	jQuery("#tcw").click(function(){
		jQuery(this).removeClass('inputcolordefault');
		if (jQuery(this).val() == jQuery(this).attr('deftxt')) jQuery(this).val('');
		if (jQuery('#fcw').val().replace(jQuery('#fcw').attr('deftxt'),'').length == 0) {
			jQuery('#tcw_panel .pop_cityBegin').css('display','block');
			jQuery('#tcw_panel .pop_cityEnd').css('display','none');
		} else {
			jQuery('#tcw_panel .pop_cityBegin').css('display','none');
			jQuery('#tcw_panel .pop_cityEnd').css('display','block');
		}
		lwkai.maskSelect('tcw_panel');
	});
	/* close btn */
	jQuery("#divPlaceEndCloseBtn_1,#divPlaceEndCloseBtn_2").click(function(){
		lwkai.clearMaskSelect('tcw_panel');
		lwkai.inputDefTxt('tcw','inputcolordefault');
	});
	jQuery("html").bind("click", function(){ 
		lwkai.clearMaskSelect("tcw_panel");
		lwkai.inputDefTxt('tcw','inputcolordefault');
	});
	
	jQuery("#tcw_panel dl dd a").click(function(){
		var val = jQuery(this).text();
		var oldval = jQuery('#tcw').val();
		lwkai.clearMaskSelect('tcw_panel');
		val = val ? val.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		oldval = oldval ?  oldval.replace(/^\s+/img,'').replace(/\s+$/img,'') : '';
		if (val != oldval) {
			jQuery("#tcw").val(val);
			jQuery('#tcw').change();
		}
	});
	
	jQuery("#fcw_panel").bind("click", function(event){event.stopPropagation();});
	jQuery("#fcw").bind("click", function(event){event.stopPropagation();});
	  
	jQuery("#tcw_panel").bind("click", function(event){event.stopPropagation();});
	jQuery("#tcw").bind("click", function(event){event.stopPropagation();});
	
	/*  下拉列表用户改变时改变默认文字颜色 */
	jQuery('#d2').change(function(e) {
        jQuery(this).val() == '' ? jQuery(this).addClass('inputcolordefault') : jQuery(this).removeClass('inputcolordefault');
    });
	jQuery("#txtkey").click(function(){
		var defTxt = jQuery(this).attr('deftxt');
		if (jQuery(this).val() == defTxt) jQuery(this).val('');
		jQuery(this).removeClass('inputcolordefault');
	});
	jQuery("#txtkey").change(function(){lwkai.inputDefTxt('txtkey','inputcolordefault');});
	
	
	/*  整个首页的 tab 鼠标切换事件  lwkai.resizeMaskSelect 是针对IE6下 子框架的 tab 切换重置事件 调用前需加载 search.js 文件*/ 
	lwkai.tabs(".tabTits","current",".tabCons",lwkai.resizeMaskSelect);
});


