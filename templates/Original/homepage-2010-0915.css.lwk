body, ul, ol, dl, dd, h1, h2, h3, h4, h5, h6, p, form, fieldset, legend, input, textarea, select, button, th, td { margin:0; padding:0; }
img { border:0; }
ul, li, ol { list-style:none; }
body { background:url(/image/head/body_bg.jpg) repeat-x top left #FFFFFF; font-size:12px; font-family:"\5b8b\4f53", SimSun, Tahoma, Arial, Helvetica, sans-serif; color:#111; }
#wrap { min-width:1003px; width:100%; overflow:hidden; }
a { color:#00388A; text-decoration: none; }
a:hover { color:#111; text-decoration:underline; }
a.blue { color:#327EF6; }
h1, h2, h3, h4, h5, h6 { font-size:12px; }
h1 { font-size: 16px; }
.clear { clear:both; }
/*************************** 下面是通用部分 ***************************/
/*验证表单的样式表*/
input.required, textarea.required { border: 1px solid #00A8E6; padding:2px 3px; height:16px; }
input.validation-failed, textarea.validation-failed { color:#111; }
input.validation-passed, textarea.validation-passed { border:1px solid #00A8E6; }
.text_time { width:130px; height:16px; padding:2px 15px 2px 3px; border:1px solid #ddd; background:url(/image/icons/Calendar.gif) no-repeat right center; line-height:16px; }
.validation-advice { margin-left:5px; padding-left:15px; width:auto; background:url(/image/icons/tip_error.gif) 0 0px no-repeat; line-height:22px; *line-height:16px;
color:#777; }
.validation-sms_send { width:auto; height:15px; line-height:15px; filter:alpha(opacity=85); -moz-opacity:0.85; color :green; font-size:12px; border: 1px solid green; background-color: #FEF4ED; margin:0px 0px 0px 5px; padding:1px; }
.validation_right img { padding:5px 0 0 5px; }
input.text { height:18px; border:1px solid #d5d5d5; line-height:18px; color:#111; padding:2px 3px; font-size:12px; }
input.textTime { background: url(/image/icons/Calendar.gif) no-repeat scroll 75px center transparent; border: 1px solid #D5D5D5; height: 18px; line-height: 18px; padding: 2px 15px 2px 3px; width: 75px; }
select { border:1px solid #d5d5d5; font-size:12px; }
textarea.textarea { border:1px solid #d5d5d5; line-height:18px; padding:3px; font-size:12px; }
/* 通用标题 */
.title { width:100%; height:27px; background:url(/image/title_bg.gif);overflow:hidden; }
.title b { float:left; left:0; top:0; width:5px; height:27px; background:url(/image/title_bg.gif); }
.title span { float:right; right:0px; top:0px; width:5px; height:27px; background:url(/image/title_bg.gif); }
.title h3 { float:left; height:24px; padding:3px 0 0 6px; font-weight:bold; line-height:20px; *line-height:23px; }
/* 蓝色背景的标题 */
.titleBig { background-position:0 0; }
.titleBig b { background-position:0 -29px; }
.titleBig span { background-position:0px -58px; }
.titleBig h3 { font-size:14px; color:#e8f6ff; }
/* 浅蓝色蓝色背景的标题 */
.titleSmall { background-position:0 -87px; }
.titleSmall b { height:27px; background-position:0 -116px; }
.titleSmall span { height:27px; background-position:0 -145px; }
.titleSmall h3 {
*margin-top:1px; _margin-top:0px; font-size:12px; color:#111; }
.titleSmall h3.orange { font-size:12px; color:#f7860f; }
.titleSmall p { float:right; *margin-top:1px;
_margin-top:0px; height:27px; color:#223D6C; line-height:27px; }
/* 圆角上部 */
.titleDetailTop { height:5px; background-position:0 -174px; font-size:1px; }
.titleDetailTop b { height:5px; background-position:0px -181px; }
.titleDetailTop span { height:5px; background-position:0px -188px; }
/* 黄色背景的标题 */
.titleYellow { background-position:0 -216px; }
.titleYellow b { background-position:0 -245px; }
.titleYellow span { background-position:0 -274px; }
/* 圆角下部 */
.titleDetailBot { margin-bottom:20px; height:5px; background-position:0 -195px; font-size:1px; }
.titleDetailBot b { height:5px; background-position:0 -202px; }
.titleDetailBot span { height:5px; background-position:0 -209px; }
/* button */
.btn { display:inline-block; height:23px; white-space:nowrap; overflow:hidden; line-height:23px; *line-height:25px;
cursor:pointer; zoom:1;position:relative;top:-1px;*top:-3px;}
.btn button { width:80px; background:none; border:0; height:22px; font-size:12px; cursor:pointer; text-align:center; overflow:visible; }
/**:lang(zh) .btn button{ padding:0 7px !important;} */
.btn:hover { background:url(/image/button_bg.gif) 0px -23px; text-decoration:none; border:1px solid #f8b709; }
/*橙色button*/
.btnOrange { background:url(/image/button_bg.gif) left top; font-weight:bold; border:1px solid #f8b709; }
.btnOrange button { font-weight:bold; }
/*灰色button*/
.btnGrey { width:auto; background:url(/image/button_bg.gif) 0px -46px; border:1px solid #e4e4e4; }
.btnCenter { clear:both; height:25px; text-align:center; }
/*A button*/
.btnA { display:inline-block; width:90px; height:21px; padding:1px 0; text-align:center; line-height:23px; cursor:pointer; }
.btnA:hover { text-decoration:none; }
.btnCenter { clear: both; height: 25px; text-align: center;}
.btnCenter .btn{position:static;}
/*兰色A button*/
.btnABlue { border:1px solid #76C9D9; color:#0270AF; background:-webkit-gradient(linear, left top, left bottom, from(#DCF7FE), to(#9ADCEA)); background:-moz-linear-gradient(top, #DCF7FE, #9ADCEA); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#DCF7FE', endColorstr='#9ADCEA');
}
.btnABlue:hover { color:#0270AF; background:-webkit-gradient(linear, left top, left bottom, from(#9ADCEA), to(#DCF7FE)); background:-moz-linear-gradient(top, #9ADCEA, #DCF7FE); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#9ADCEA', endColorstr='#DCF7FE');
}
/* 返回顶部按钮 */
/*.goTop{ float:right; margin:5px 20px 0 0; display:inline; width:46px; height:21px;  }*/
.goTop { text-align:right }
.goTop a { display:inline-block; width:46px; height:21px; background:url(/image/go_top.jpg); }
/* 分页 */
.page { float:left; margin-left:16px; display:inline; width:430px; height:18px; line-height:16px; color:#00388a; font-family:"宋体"; }
.page a { color:#00388a; line-height:14px; }
.page a.go { float:left; padding:2px 3px; height:14px; background:url(/image/page_bg.gif) no-repeat; }
.page a.first { width:8px; background-position: 2px 3px; *background-position: 2px 2px;
}
.page a.pre { width:40px; margin-left:11px; display:inline; padding-left:11px; background-position:-33px 3px; *background-position:-33px 2px;
}
.page a.last { width:8px; margin-left:11px; display:inline; background-position:2px -16px; *background-position:2px -17px;
}
.page a.next { width:40px; margin-left:11px; display:inline; padding-right:10px; background-position:-33px -16px; *background-position:-33px -17px;
}
.page span { float:left; height:18px; }
.page span a { padding:0 4px; line-height:16px; }
.page span b { padding:1px 7px 0; background:#128DDA; color:#fff; }
/* 网站地图 */
.pathLinks { padding:5px 0 5px 8px; width:500px; height:16px; line-height:16px; color:#111; }
.pathLinks a { color:#223c6a; }
.pathLinks i { font-style:normal; color:#777; }
/* 选项tab */
.chooseTab { clear:both; position:relative; width:745px; height:32px; background:url(/image/route_tab.gif); }
.chooseTab li { position:relative; float:left; padding-left:8px; margin-right:3px; display:inline; background:url(/image/route_tab.gif) 0 -68px no-repeat; cursor:pointer; }
.chooseTab li a { display:block; float:left; height:26px; padding:6px 17px 0; background:url(/image/route_tab.gif) 0 -34px; font-size:14px; line-height:26px; color:#111; }
.chooseTab li a:hover { color:#111; text-decoration:none; }
.chooseTab li span { display:block; float:left; width:8px; height:32px; background:url(/image/route_tab.gif) 0 -102px; }
.chooseTab li.selected { background-position:left -170px; }
.chooseTab li.selected a { background-position:right -136px; padding:6px 11px 0; font-size:16px; font-weight:bold }
.chooseTab li.selected span { background-position:left -204px; }
#content { width:950px; margin:0 auto; padding:10px 0 8px; background:#fff; overflow:hidden; }
/*************************** 通用部分结束 ***************************/

/* 头部信息 */

#head { width:950px; margin:0 auto; }
#headTop { width:950px; height:110px; }
/* 头部顶部 */
.headInfo { padding-top:5px; width:950px; height:20px; line-height:20px; }
.headInfo ul { float:right; }
.headInfo .info { width:192px; }
.headInfo .info li { float:left; color:#A5C6E6; }
.headInfo .info li { margin-left:12px; display:inline; }
.headInfo .info li.us { font-family:Tahoma; }
.headInfo .info li a { color:#A5C6E6; }
.headInfo .info li a:hover { color:#fff; }
.headInfo .info li.helpC { margin-left:0; margin-right:27px; display:inline; padding-left:18px; background: url(/image/icons/helpblue.gif) no-repeat left 2px; }
.headInfo .info li.helpC a { color:#DDEEFF; line-height:20px; *line-height:22px;
_line-height:20px; }
.headInfo .login { margin-right:36px; display:inline; }
.headInfo .login li { float:right; color:#DDEEFF; }
.headInfo .login li a { color:#FFD52D; padding:0 5px; margin:0 2px; }
.headInfo .login li.news { margin-right:5px; color:#FFD52D; }
.headInfo .login li.news a { margin:0; padding-left:16px; background:url(/image/icons/News_receive.gif) left center no-repeat; }
/* logo */
#headTop .logo { clear:both; float:left; width:260px; height:55px; }
#headTop .logo h3 { float:left; margin-left:7px; display:inline; width:184px; height:72px; }
#headTop .logo h4 { float:left; margin-top:25px; font-weight:normal; color:#E7EEFE; }
/* 搜索 */
.search { position:relative; float:left; margin:20px 0 0 18px; display:inline; width:380px; height:32px; background:#75AAEE; z-index:10; }
.searchLeft { position:relative; float:left; height:32px; width:296px; padding-left:4px; background:url(/image/head/search_bg.gif) no-repeat; }
.searchRight { float:left; height:32px; width:80px; background:url(/image/head/search_bg.gif) right -32px no-repeat; }
.searchInput { width:288px; height:16px; line-height:16px; color:#999; margin-top:3px; *margin-top:2px;
border:0px; padding:5px 4px; display:inline; }
.searchSubmit { margin:3px 0 0 4px; width:74px; height:27px; background:url(/image/head/btn_search.gif) no-repeat top left; cursor:pointer; border:none; }
