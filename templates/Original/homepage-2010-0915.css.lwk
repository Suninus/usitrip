}
.popupTable td.topRight { width:6px; height:6px; background:url(/image/popup/popup_t3.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_t3.png', sizingMethod=scale);
}
.popupTable td.botLeft { height:6px; background:url(/image/popup/popup_b1.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_b1.png', sizingMethod=scale);
}
.popupTable td.botRight { background:url(/image/popup/popup_b3.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_b3.png', sizingMethod=scale);
}
.popupTable td.con { background:#fff; }
.popupMin { position:absolute; right:25px; top:3px; width:14px; height:14px; border:1px solid #888; color:#888; text-align:center; font-size:15px; line-height:14px; cursor:pointer; }
/* 弹出层的遮盖层 */
#DivShim { position:absolute; filter:mask(); border:none; display:none; }
/* 弹出框按钮 */
.popupCon { padding:12px; text-align:left; overflow:hidden; }
.popupConTop { width:100%; height:25px; border-bottom:1px #DBDBDB dashed; position:relative; }
.popupConTop h3 { float:left; height:25px; line-height:25px; color:#777; font-weight:normal; }
.popupConTop h3 b { font-size:14px; color:#111; }
.popupConTop h3 label { color:#F7860F; font-size:12px; font-weight:normal; }
.popupCon .popupConTop span { position:absolute; right:0; top:3px; width:16px; height:16px; cursor:pointer; background: url("/image/popup/icon_x.gif") repeat scroll 0 0 transparent; }
.popupBtn { clear:both; height:25px; margin:0 auto; padding:15px 0; text-align:center; }
.popupBtn .btn { margin:0 10px; }
/* 弹出层成功提示 */
.popup .successTip .img { float:left; width:64px; height:64px; margin-left:15px; display:inline; }
.popup .successTip .words { margin-left:80px; padding-top:25px; line-height:18px; text-align:left; }
.popup .successTip .words div { color:#ff7200; text-align:center;}
.popup .successTip p { padding-bottom:10px; line-height:18px; }
.popup .successTip span { color:#F7860F; }
.popup .successTip span b { font-family:Tahoma; }
/* 弹出层登录 */
.popup .login { margin:10px 0 0 20px; overflow:hidden; color:#777; line-height:22px; }
.popup .login a { text-decoration:underline; }
.popup .login li { padding:5px 0; }
.popup .login label { display:inline-block; width:80px; text-align:right; vertical-align:top; }
.popup .login .text { display:inline-block; width:150px; }
.popup .loginBtn { width:82px; height:27px; background:url(/image/button_login.gif); border:0; color:#223D6C; font-weight:bold; font-size:14px; cursor:pointer; }
/* 新弹出层 */
.pop { display:none; position:absolute; text-align:center; z-index:100; }
#popBg { position:absolute; z-index:99; top:0; left:0; background:#ccc; filter:alpha(opacity=30); -moz-opacity:0.3; opacity:0.3; }
.popBgIframe { filter:alpha(opacity=0); -moz-opacity:0; opacity:0; }
.popTable td.side { background:url(/image/popup/popup_png.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_png.png', sizingMethod=scale);
}
.popTable td.topLeft { width:6px; height:6px; background:url(/image/popup/popup_t1.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_t1.png', sizingMethod=scale);
}
.popTable td.topRight { width:6px; height:6px; background:url(/image/popup/popup_t3.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_t3.png', sizingMethod=scale);
}
.popTable td.botLeft { height:6px; background:url(/image/popup/popup_b1.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_b1.png', sizingMethod=scale);
}
.popTable td.botRight { background:url(/image/popup/popup_b3.png); _background:none; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/image/popup/popup_b3.png', sizingMethod=scale);
}
.popTable td.con { background:none; _filter:none; background:#fff; }
/* 新弹出框按钮 */
.btnPop { margin:15px 0 0 180px; display:inline; }
.popCon { padding:12px; text-align:left; overflow:hidden; }
.popCon .popTitle { width:100%; height:25px; border-bottom:1px #DBDBDB dashed; position:relative; }
.popCon .popTitleCon { font-size:14px; color:#111; line-height:25px; }
.popCon .popClose { position:absolute; right:0; top:3px; width:16px; height:16px; cursor:pointer; background:url(/image/icons/icon_x.gif); }
#popupIng{width:386px;}
/* 数据提交后的提示 */
.messageBox { font-size: 12px; }
.messageStackError, .messageStackWarning { font-size: 12px; background-color: #ffb3b5; padding:2px; }
.messageStackSuccess { font-size: 12px; background-color: #99ff00; padding-top:5px; padding-bottom:5px; padding-left:3px; }
/*成功提示*/
.successTip .img { float:left; width:64px; height:64px; margin-left:15px; display:inline; }
.successTip .words { margin-left:80px; padding-top:25px; line-height:18px; text-align:left; }
.successTip p { padding-bottom:10px; line-height:18px; }
.successTip span { color:#F7860F; }
.successTip span b { font-family:Tahoma; }
/* 输入框默认文字颜色 */
input.texttip, textarea.texttip { color: #CCC; }
/*sold out tips*/
.bookMark { margin:20px 0 10px; }
.bookMark p { margin-bottom:10px; }
.bookMark span.center { display:block; text-align:center; }
.bookMark .text { width:200px; }
/* 服务器更新提示  */
#ServerSuspendTip { height:37px; background:-webkit-gradient(linear, left top, left bottom, from(#fffac2), to(#f8f099)); background:-moz-linear-gradient(top, #fffac2, #f8f099); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#fffac2', endColorstr='#f8f099');
}
#ServerSuspendTip .con { width:950px; height:37px; margin:0 auto; line-height:37px; }
#ServerSuspendTip #ServerSuspendTipCon { float:left; width:920px; height:37px; }
#ServerSuspendTip .con .close { float:right; width:26px; height:26px; margin-top:5px; background:url(/image/close_red.gif); cursor:pointer; }
/*版本切换工具条*/
.switchVersion { width:950px; height:33px; margin:0 auto; border-top:1px solid #e1e1e1; border-bottom:1px solid #e1e1e1; background:#f4f4f4; line-height:33px; text-align:center; }
.switchVersion p { display:inline-block; *display:inline;
*zoom:1;
vertical-align:middle; }
.switchVersion .switchVersionBtn { display:inline-block; *display:inline;
*zoom:1;
width:96px; height:23px; margin-left:10px; background:url(/image/switch_version_bg.gif); line-height:23px; color:#fff; text-align:center; }
.switchVersion .switchVersionBtn:hover { color:#fff; text-decoration:none; }
/*酒店页面星级显示*/
.proListCon .mid li h4 { float:left; margin:0; }
p.stars0, p.stars1, p.stars2, p.stars3, p.stars4, p.stars5 { width:59px; height:11px; line-height:10px; margin-top:6px; *margin-top:4px;
_margin-top:6px; text-indent:-9999px; }
.proDes .proDesDetail .mid p.stars0, .proDes .proDesDetail .mid p.stars1, .proDes .proDesDetail .mid p.stars2, .proDes .proDesDetail .mid p.stars3, .proDes .proDesDetail .mid p.stars4, .proDes .proDesDetail .mid p.stars5 { float: left; width:59px; height:11px; line-height:10px; text-indent:-9999px; padding:0; margin-top:6px; *margin-top:4px;
_margin-top:6px; }
p.stars0 { background:url(/image/stars.gif) 0 -55px no-repeat; }
p.stars1 { background:url(/image/stars.gif) 0 -44px no-repeat; }
p.stars2 { background:url(/image/stars.gif) 0 -33px no-repeat; }
p.stars3 { background:url(/image/stars.gif) 0 -22px no-repeat; }
p.stars4 { background:url(/image/stars.gif) 0 -11px no-repeat; }
p.stars5 { background:url(/image/stars.gif) 0 0 no-repeat; }
/*酒店页面左侧导航*/
.content_category .proLeft .newNav dd { width:173px; }
.content_category .proLeft .newNav dd a { width:163px; }
.tipslayer { position: relative; z-index: 0; }
.tipslayer:hover { background-color: transparent; z-index: 50; }
.tipslayer span { /*CSS for enlarged image*/ position:absolute; background-color: white; padding: 5px; /*left: -1000px;*/ border:1px solid #52AADE; visibility: hidden; color: black; text-decoration: none; opacity:.95; filter:alpha(opacity=95); }
.tipslayer:hover span { /*CSS for enlarged image on hover*/ visibility: visible; top: -110px; width:280px; left:-100px !important; left:-100px; cursor:pointer;/*left: 60px; position where enlarged image should offset horizontally */ }
