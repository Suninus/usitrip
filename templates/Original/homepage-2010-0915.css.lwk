.searchAdvanced .top1 b { float:left; width:4px; background:url(/image/head/search_advanced_top_left.png); _background:none; _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true', sizingMethod='scale', src="/image/head/search_advanced_top_left.png");
}
