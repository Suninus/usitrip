.searchAdvanced .top1 span { float:left; width:4px; background:url(/image/head/search_advanced_top_right.png); _background:none; _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true', sizingMethod='scale', src="/image/head/search_advanced_top_right.png");
}
/* 高级搜索链接 */
.advanced { float:left; margin-top:20px; padding:9px; width:58px; z-index:1000 }
.advanced a { color:#BDDCFF; }
.advanced a:hover { color:#FFF; }
/* 高级搜索内容 */
.searchAdvanced .con { padding:10px; width:352px; overflow:hidden; background:#fff; border-left:4px solid #74A9EE; border-right:4px solid #74A9EE; }
.searchAdvanced .con h2 { float:left; padding-bottom:5px; width:200px; height:16px; line-height:16px; font-size:14px; font-weight:bold; color:#111; }
.searchAdvanced .con span { float:right; width:16px; height:16px; }
.searchAdvanced .list { clear:both; width:100%; padding-top:10px; }
.searchAdvanced .list b { float:left; width:55px; color:#777; line-height:22px; font-weight:normal }
.searchAdvanced .list .text1 { float:left; margin-right:8px; display:inline; width:102px; }
.searchAdvanced .list .text2 { float:left; margin-right:2px; display:inline; width:36px; }
.searchAdvanced .list select.select { float:left; margin-right:8px; display:inline; width:110px; }
.searchAdvanced .list i { float:left; margin:0 2px; display:inline; width:15px; line-height:22px; }
.searchAdvanced .list .time { float:left; padding-right:15px; width:70px; background:url(/image/icons/Calendar.gif) no-repeat 70px center; }
.searchAdvanced .list .keyword { float:left; width:165px; }
.searchAdvanced .list .submit { width:75px; height:28px; background:url(/image/head/btn_search_advanced.gif) no-repeat top left; cursor:pointer; border:none; margin:5px 0; margin-left:140px; }
.searchAdvanced .bot b { float:left; width:4px; height:3px; font-size:1px; background:url(/image/head/search_advanced_bot_left.png); _background:none; _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true', sizingMethod='scale', src="/image/head/search_advanced_bot_left.png");
}
.searchAdvanced .bot p { float:left; width:372px; height:3px; font-size:1px; background:#74A9EE; }
.searchAdvanced .bot span { float:left; width:4px; height:3px; font-size:1px; background:url(/image/head/search_advanced_bot_right.png); _background:none; _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true', sizingMethod='scale', src="/image/head/search_advanced_bot_right.png");
}
/* 自动完成提示 */
.recommend { position:absolute; background:#fff; left:3px; top:29px; border:1px solid #75AAEE; z-index:100; }
.recommend ul li { line-height:24px; padding-left:10px; display:block; }
.recommend ul li:hover { display:block; background:#3684C6; color:#FFF; text-decoration: none; cursor:pointer; }
/* 电话 */
.servicePhone { float:right; margin-top:15px; width:192px; }
.servicePhone li { float:left; background:url(/image/icons/Phone.gif) no-repeat left 2px; padding-left:18px; height:20px; font-size:16px; color:#fff; font-weight:bold; font-family:Arial; line-height:20px; }
.servicePhone li i { font-style:normal; font-size:14px; }
.servicePhone li em { font-family:Simsun, sans-serif; font-size:12px; font-style:normal; font-weight:normal; }
.servicePhone li.us em { padding-left:8px; }
/* 导航菜单 */
.headMenu { height:34px; background:url(/image/head/head_menu_bg.jpg); }
.headMenu li { float:left; padding:0 2px; background:url(/image/head/head_menu_bg.jpg) no-repeat right -102px; line-height:34px; }
.headMenu .headMenuLeft { background:url(/image/head/head_menu_bg.jpg) left -34px no-repeat; height:34px; width:4px; padding:0; }
.headMenu .headMenuRight { float:right; background:url(/image/head/head_menu_bg.jpg) right -68px no-repeat; height:34px; width:4px; padding:0; }
.headMenu li a { display:block; float:left; color:#fff; height:34px; padding:0 9px; }
.headMenu li a:hover { background:url(/image/head/head_menu_bg.jpg) 0px -136px; color:#111; text-decoration:none; }
.headMenu li.new { position:relative; }
.headMenu li.new .newIcon { position:absolute; top:-5px; right:-10px; width:28px; height:15px; background:url(/image/icons/new_icon.gif); font-size:0; }
.headMenu .sel a { color:#111; background:url(/image/head/head_menu_bg.jpg) repeat-x left -136px; text-decoration:none; font-weight:bold; font-size:14px; }
.headMenu li.group { float:right; margin-right:5px; display:inline; background:none; }
.headMenu li.group a { display:inline; background:url(/image/head/menu_order_bg.jpg); width:69px; height:20px; margin-top:5px; padding:4px 0 0 8px; line-height:18px; *line-height:20px;
color:#C7E5FD; }
.headMenu li.group a:hover { background: url(/image/head/menu_order_bg.jpg) 0px -24px; color:#fff; text-decoration: none; }
/* 导航菜单底部 */
.toolBar { width:930px; height:34px; padding:0px 10px 0px; background:url(/image/head/head_menu_bg.jpg) 0px -170px; line-height:32px; }
.toolBar .subCategory dt, .toolBar .subCategory dd { float:left; padding: 0 5px; }
.toolBar .subCategory dt { color:#E88E00; *line-height:30px;
_line-height:32px }
.toolBar .subCategory dd a { color:#111; }
.toolBar .subCategory dd a:hover { color:#00388A; }
/* 导航菜单底部右边 */
.tool { float:right; margin:4px 0; display:inline; width:295px; height:22px; background:url(/image/head/head_menu_bg.jpg) 0px -204px; border:1px solid #FFCD55; line-height:22px; color:#114083; }
.tool a:hover { text-decoration:none; color:#cf5300; }
.tool .myCart, .tool .myTours, .tool .integral { float:left; margin-left:20px; display:inline; color:#114083; }
.tool .myCart { margin-left:17px; display:inline; padding-left:18px; background:url(/image/icons/cart.gif) no-repeat left 2px; *padding-top:2px;
*line-height:21px;
_padding-top:1px; }
.tool .myCart span { color:#E13500; font-weight:bold; }
/* 我的八八八 */
.tool .myTours { position:relative; margin-left:25px; display:inline; padding:0 10px 0 13px; *padding:3px 10px 0 13px;
line-height:22px; *line-height:20px;
_height:19px; _line-height:19px; width:62px; background:url(/image/head/my_tours.jpg) no-repeat right 0px; cursor:pointer; z-index:1; }
.tool .myTours2 { color:#111; width:65px; height:22px; padding:0 9px; *padding:3px 9px 0;
line-height:22px; *line-height:20px;
_height:19px; _line-height:19px; background:url(/image/head/my_tours.jpg) no-repeat right -22px; cursor:pointer; display:inline; border-left:1px solid #FFC155; border-right:1px solid #FFC155; font-weight:bold; }
.myAnother { position:absolute; width:83px; border:1px solid #FFC155; border-top:0px; top:22px; right:-1px; _right:-2px; background:#FFF; text-align:center; }
.myAnother li { float:left; border-top:1px solid #eff1f5; background:#FFF; text-align:center; }
.myAnother li a { display:block; width:83px; height:22px; line-height:22px; font-weight:normal; }
.myAnother li a:hover { background:#ffefcf; }
.tool .integral { padding-left:5px; *padding-top:2px;
*line-height:21px;
_padding-top:1px; }
/* 预订指南 */
.guid { clear:both; margin:10px auto; width:930px; border-top:1px solid #D4D4D4; background:#EFF9FF; overflow:hidden; }
.guid h4 { text-indent:10px; float:left; padding:24px 0 5px; font-size:14px; }
.guid img { float:right; padding:8px 4px 8px 0px; }
.guid p { float:right; padding:10px 5px 10px 0px; }
.guid a { display:block; float:left; }
.guid a.step1 { width:204px; height:48px; background: url(/image/guid-main-img.gif) left top; }
.guid a.step2 { width:218px; height:48px; background: url(/image/guid-main-img.gif) -204px 0px; }
.guid a.step3 { width:209px; height:48px; background: url(/image/guid-main-img.gif) -422px 0px; }
.guid a.step4 { width:148px; height:48px; background: url(/image/guid-main-img.gif) right top; }
a.step1:hover { background: url(/image/guid-main-img.gif) left bottom; }
a.step2:hover { background: url(/image/guid-main-img.gif) -204px -48px; }
a.step3:hover { background: url(/image/guid-main-img.gif) -422px -48px; }
a.step4:hover { background: url(/image/guid-main-img.gif) right bottom; }
/* foot */
#foot { clear:both; width:950px; margin:auto; padding:10px 0; line-height:19px; text-align:left; overflow:hidden; }
.friendLink { margin-top:10px; text-align:left; clear:both; margin-bottom:10px; background:url(/image/head/friend_link.gif) repeat-x top left; }
.friendLink div { background:url(/image/head/friend_link.gif) repeat-x bottom left; padding:3px 0px 3px 0px; }
.footF { text-align:left; color:#627FAF; font-size:12px; float:left; width:615px; margin-bottom:10px; }
.footF span, .footF a { color:#627faf }
